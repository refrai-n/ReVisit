{
    "manifest_version": 3,
    "name": "ReVisit",
    "version": "1.0",
    "description": "ReVisit the last url from history of a specific website",
    "author": "Refrain",
    "permissions": [
        "history",
        "storage",
        "tabs"
    ],
    "action": {
        "default_popup": "index.html"
        
    },
    "background": {
        "service_worker": "scripts/background.js"
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    }
}